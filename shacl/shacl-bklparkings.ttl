@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh:	<http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix ex:	<http://donostia.eus/linkeddata/id/shacl-bklparkings> .
@prefix lod: <http://donostia.eus/linkeddata/id/> .
@prefix w3-time: <http://www.w3.org/2006/>.


ex:ParkingShape
	a sh:NodeShape ;
	sh:targetClass schema:ParkingFacility ;
	sh:property [
	sh:path rdfs:label ;
	sh:minCount 1 ;
	sh:message "Una estacion de bicis debe tener un label@es";
	sh:severity sh:Violation ;
	];
	
	sh:property [
	sh:path lod:Number_of_routes ;
	sh:minCount 1 ;
	sh:maxCount 1 ;
	sh:datatype xsd:float ;
	sh:message "Una estacion de bicis debe tener un valor para el nº de rutas@es" ;
	sh:severity sh:Violation ;
	];
	sh:property [
	sh:path lod:Users_quantity ;
	sh:minCount 1 ;
	sh:maxCount 1 ;
	sh:datatype xsd:float ;
	sh:message "Un parking debe tener un valor para el nº de numero de usuarios@es" ;
	sh:severity sh:Violation ;
	];
	sh:property [
	sh:path lod:Average_duration ;
	sh:minCount 1 ;
	sh:maxCount 1 ;
	sh:datatype xsd:float ;
	sh:message "Un parking debe tener un valor para el nº de duración media@es" ;
	sh:severity sh:Violation ;
	].
	


