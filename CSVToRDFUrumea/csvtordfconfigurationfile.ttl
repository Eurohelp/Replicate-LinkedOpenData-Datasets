@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix schema: <http://schema.org/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix linkedgeodata: <http://linkedgeodata.org/page/ontology/> .
@prefix qb: <http://purl.org/linked-data/cube#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix lod: <http://lod.eurohelp.es/def/medio-ambiente/medicion/> .

#Se define por cada sujeto un bloque en el que se podrán anadir todos los 
#predicados y objetos necesarios
<#Urumea>
rml:logicalSource [
		rml:source "newdata/txominea.csv";
        rml:referenceFormulation ql:CSV
    ];
 		rr:subjectMap [
        rr:constant "http://lod.eurohelp.es/id/river/urumea/txominea";
        rr:class schema:RiverBodyOfWater;
    ];
    	rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [
            rr:constant "Rio Urumea";
        ]
        ];
        
        rr:predicateObjectMap [
        rr:predicate lod:hasObservation;
        rr:objectMap [
        rr:template "http://lod.eurohelp.es/recurso/medio-ambiente/rio/txominea-{eguna}-{horaParaUri}";
        ]
        ]
        .
        
<MedicionConcreta>
rml:logicalSource [
		rml:source "newdata/txominea.csv";
        rml:referenceFormulation ql:CSV
    	];
 		rr:subjectMap [
        rr:template "http://lod.eurohelp.es/recurso/medio-ambiente/rio/txominea-{eguna}-{horaParaUri}";
        rr:class qb:Observation;
    	];
    	rr:predicateObjectMap [
        rr:predicate dcterms:date;
        rr:objectMap [
            rml:reference "horafecha";
            rr:datatype xsd:dateTime
        ]
        ];
        rr:predicateObjectMap [
        rr:predicate "http://lod.eurohelp.es/def/medio-ambiente/medicion/riesgo";
        rr:objectMap [
            rml:reference "arriskua";
            rr:language "vq";
        ]
        ];
        rr:predicateObjectMap [
        rr:predicate "http://lod.eurohelp.es/def/medio-ambiente/medicion/nivel-agua";
        rr:objectMap [
            rml:reference "maila";
            rr:datatype xsd:float;
        ]
        ];
        rr:predicateObjectMap [
        rr:predicate "http://lod.eurohelp.es/def/medio-ambiente/medicion/unidad-medida-nivel-agua";
        rr:objectMap [
            rr:constant "metros";
        ]
        ].